{
  "rules": {
    ".read": false,
    ".write": false,
    "permissions": {
      "user": {
        "$uid": {
          ".read": "auth != null && $uid === auth.uid"
        }
      }
    },
    "trails": {
      ".read": true,
      "$trailId": {
        ".write": "root.child('permissions/user/' + auth.uid + '/trails_modify').exists()"
      }
    }
  }
}
